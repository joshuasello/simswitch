.PARAM  fpar126=864  fpar127=0  fpar128=0
.PARAM  fpar129=0.001
.FUNC T()          {V(Tj)}
.FUNC mu(T)        {fpar123 + fpar124*T + fpar125*T**2}
.FUNC Res(T)       {length/(fpar122*mu(T)*fpar44) + contacts*fpar121/fpar44}
.FUNC Isat(T)      {fpar44*(fpar126 + fpar127*(T-25) + fpar128*(T-25)**2)}
